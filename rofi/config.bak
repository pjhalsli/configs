
! Enable the extended coloring options
rofi.color-enabled: true

! Property Name     BG       Border   Separator
rofi.color-window:  #181818, #031619, #021012

! Property Name     BG       FG       BG-alt   Head-BG  Head-FG
rofi.color-normal:  #181818, #C3C7BF, #181818, #3E4146, #C3C7BF
rofi.color-active:  #181818, #ffffff, #A2A8A3, #87979C, #ffffff
rofi.color-urgent:  #181818, #C3C7BF, #181818, #181818, #C3C7BF

! Set the desired separator style
rofi.separator-style: solid
